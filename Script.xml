<MyScript>





	<Properties>
		<cacheSize>50</cacheSize>
		<domain>http://www.customercentrix.com</domain>
		<jobID>0001</jobID>
		<timeout>5000</timeout>
		<waitstep>1000</waitstep>
		<iteration>1</iteration>
		<duration>60000</duration>
		<proxyPort>8099</proxyPort>
		<proxyHost>none</proxyHost>
		<javaScriptEnabled>false</javaScriptEnabled>
		<redirectEnabled>true</redirectEnabled>
		<throwExceptionOnScriptError>true</throwExceptionOnScriptError>
		<useInsecureSSL>false</useInsecureSSL>
		<popupBlockerEnabled>true</popupBlockerEnabled>
		<htmlRobotBrowserVersion>none</htmlRobotBrowserVersion>
	</Properties>

	<STEPS>
		<WAIT length="3000" />
		<INVOKE path="" ></INVOKE>
		<VERIFY_TITLE title="CustomerCentrix | We satisfy your business technology needs" ></VERIFY_TITLE>
		
		<SET_INPUT_FIELD name="test_user" pass="test_pass" op="Log In"/><POST path=""></POST><VERIFY_TITLE title="user account | CustomerCentrix"></VERIFY_TITLE><WAIT
			length="3000">
</WAIT>
		
		<FILL_FORM username="test_user" ></FILL_FORM>
		<WAIT length="3000"></WAIT>
		
	</STEPS>

</MyScript>